{
  "installCommand": "npm install",
  "buildCommand": "npm --workspace apps/frontend run build",
  "outputDirectory": "apps/frontend/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://ai-job-tracker1.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
